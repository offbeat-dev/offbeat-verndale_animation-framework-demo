<section class="sticky-module background">
  <div class="container">
    <div class="row">
      <div class="sticky-module__sticky-column col-md-6">
        <fade-in>
          <animated-element
            class="sticky-module__animated-square"
            opacity="0, 1"
            translate-x="200%, 0"
            scale="0.5, 1"
            rotate="45deg"
            times="0.5, 0.5, 1"
          >
