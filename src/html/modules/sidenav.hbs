    <triggered-group reverse-on-click-outside>
      <ul class="sidenav__items">
        <li class="sidenav__item">
          <button class="sidenav__menu-trigger" id="inline-menu-1">
            About Us
          </button>
          <trigger-element target="inline-menu-1" opacity="0, 1" translate-y="-100%, 0" duration="0.4" class="sidenav__megamenu-container">
            <div class="sidenav__inline-menu-item">
              <ul><li>Menu item 1</li><li>Menu item 2</li><li>Menu item 3</li></ul>
            </div>
          </trigger-element>
        </li>
        <li class="sidenav__item">
          <button class="sidenav__menu-trigger" id="inline-menu-2">
            Products
          </button>
          <trigger-element target="inline-menu-2" opacity="0, 1" translate-y="-100%, 0" duration="0.4" class="sidenav__megamenu-container">
            <div class="sidenav__inline-menu-item">
              <ul><li>Menu item 1</li><li>Menu item 2</li></ul>
            </div>
          </trigger-element>
        </li>
        <li class="sidenav__item">
          <button class="sidenav__menu-trigger" id="inline-menu-3">
            Industries
          </button>
          <trigger-element target="inline-menu-3" opacity="0, 1" translate-y="-100%, 0" duration="0.4" class="sidenav__megamenu-container">
            <div class="sidenav__inline-menu-item">
              <ul><li>Menu item 1</li></ul>
            </div>
          </trigger-element>
        </li>
      </ul>
    </triggered-group>

    <zoom-in-up delay="1">
        <triggered-element trigger-type="click" trigger-controls="sidenav__side-menu" class="sidenav__side-trigger">
          {{>animated-button animationClass="sweep-anim" customClass="sidenav__cta" label="Contact Us"}}
        </triggered-element>
    </zoom-in-up>
    {{#with animatedElement}}
    <triggered-element id="sidenav__side-menu" hide {{#if className}}class="{{className}}"{{/if}} {{#if inOpacity}}in-opacity="{{inOpacity}}"{{/if}} {{#if outOpacity}}out-opacity="{{outOpacity}}"{{/if}} {{#if inTranslateY}}in-translate-y="{{inTranslateY}}"{{/if}} {{#if outTranslateY}}out-translate-y="{{outTranslateY}}"{{/if}} {{#if inTranslateX}}in-translate-x="{{inTranslateX}}"{{/if}} {{#if outTranslateX}}out-translate-x="{{outTranslateX}}"{{/if}}{{#if duration}} duration="{{duration}}"{{/if}}{{#if easing}} easing="{{easing}}"{{/if}}>
        <div class="sidenav__menu-inner">
            <div>
                <p>Choose your solution</p>
                <ul>
                    <animated-timeline>
                        <animated-element opacity="1" duration="1.2" class="animated-opacity overflow-hidden rounded">
                            <li class="secondary-text">Optimization</li>
                        </animated-element>
                        <animated-element opacity="1" duration="1.2" at="0.4" class="animated-opacity overflow-hidden rounded">
                            <li class="secondary-text">Digital Marketing</li>
                        </animated-element>
                        <animated-element opacity="1" duration="1.2" at="0.8"  class="animated-opacity overflow-hidden rounded">
                            <li class="secondary-text">Commerce</li>
                        </animated-element>
                    </animated-timeline>
                </ul>
            </div>
            <triggered-element trigger-type="click" trigger-controls="sidenav__side-menu" class="sidenav__side-trigger">
                {{>svg name="plus" className="sidenav__menu-close"}}
            </triggered-element>
        </div>
    </triggered-element>
    {{/with}}


